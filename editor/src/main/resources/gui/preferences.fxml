<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.preferences.PreferencesController">
   <children>
      <TabPane prefHeight="768.0" prefWidth="1024.0" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab text="General">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab text="Provers">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ComboBox id="localProversCombo" layoutX="325.0" layoutY="40.0" prefHeight="26.0" prefWidth="198.0" />
                        <Button id="applyButton" onAction="#onApplyProvers" layoutX="531.0" layoutY="40.0" mnemonicParsing="false" text="Apply" />
                        <Button id="testProverCommandButton" onAction="#onTestProver" layoutX="531.0" layoutY="120.0" mnemonicParsing="false" text="Test Provers" />
                        <TextArea id="proverCommandTextArea" layoutX="14.0" layoutY="40.0" prefHeight="82.0" prefWidth="303.0" />
                        <Label layoutX="14.0" layoutY="19.0" text="Indicate filename with %f and timeout with %t" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
